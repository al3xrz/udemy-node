<div class="row">
    <div class="col s8 offset-s2 m6 offset-m3">
        <div class="card">
            <div class="card-image">
                <img src={{course.imgurl}} alt={{course.title}}>
            
            </div>
            <div class="card-content">
                <span class="card-title">{{course.title}}</span>
                <p class="price">{{course.price}}</p>
            </div>
           
        </div>
    </div>
</div>